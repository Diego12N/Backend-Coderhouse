<section class="mt-5">
	<h1 class="text-danger">Productos Disponibles</h1>
	<% if(data.length === 0) { %>
	<h2 class="bg-danger rounded text-center m-4">
		No hay productos en la lista
	</h2>
	<% } else {%>
	<table class="table table-dark">
		<tr class="text-warning">
			<th>Titulo</th>
			<th>Precio</th>
			<th>Imagen</th>
		</tr>
		<% data.forEach(function(product) { %>
		<tr>
			<td><%= product.title %></td>
			<td>$ <%= product.price %></td>
			<td><%= product.thumbnail %></td>
		</tr>
		<% }) %>
	</table>
	<% } %>
	<a href="/">Volver al formulario</a>
</section>
